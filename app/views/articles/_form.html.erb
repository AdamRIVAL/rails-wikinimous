<%= form_with model: article do |f|%>
  <div class="form-div">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="form-div">
    <%= f.label :content %>
    <%= f.text_field :content %>
  </div>
  <div class="form-div">
    <%= f.submit %>
  </div>
<% end %>
<br>
<div class="form-div">
  <%= link_to "Back to Articles", articles_path, class: "button" %>
</div>
